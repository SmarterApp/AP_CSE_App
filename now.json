{
  "public": true,
  "type": "static",
  "static": {
    "rewrites": [
      { "source": "storybook/**/!(iframe.html)", "destination": "/storybook/index.html" },
      { "source": "storybook/**/iframe.html", "destination": "/storybook/iframe.html" },
      { "source": "app/**", "destination": "/app/index.html" }
    ]
  }
}
