{
  "version": 2,
  "name": "ap-cse-app",
  "public": true,
  "alias": ["ap-cse-app.now.sh"],
  "builds": [{ "src": "./package.json", "use": "@now/static-build" }],
  "routes": [
    { "src": "^/assets/(.*)", "dest": "/assets/$1" },
    { "src": "^/vendor.js", "dest": "/vendor.js" },
    { "src": "^/main.(.*).css", "dest": "/main.$1.css" },
    { "src": "^/app.bundle.(.*).js", "dest": "/app.bundle.$1.js" },
    { "src": "^/(.*)", "dest": "/index.html" }
  ]
}
